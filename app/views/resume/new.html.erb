<div class="container">

	<h1>Create Resume</h1>



<%= form_for(:resume, :url=>{:action=>'create'}) do |form| %>
  
  <div class="form-group">
    <%= form.label :student_name %>
    <%= form.text_field :student_name, class: 'form-control', required: 'true' %>
  </div>

  <div class="form-group">
    <%= form.label :student_no %>
    <%= form.text_field :student_no, class: 'form-control', required: 'true' %>
  </div>

  <div class="form-group">
    <%= form.label :programming_skills %>
    <%= form.text_field :programming_skills, class: 'form-control', required: 'true' %>
  </div>


  <div class="form-group">
    <%= form.label :contact_information %>
    <%= form.text_field :contact_information, class: 'form-control', required: 'true' %>
  </div>


  <div class="form-group">
    <%= form.label :list_of_subjects %>
    <%= form.text_field :list_of_subjects, class: 'form-control', required: 'true' %>
  </div>

  <div class="form-group">
    <%= form.label :hobbies_and_interests %>
    <%= form.text_field :hobbies_and_interests, class: 'form-control', required: 'true' %>
  </div>


  <div class="actions">
    <%= form.submit "Create Resume", :class=>'btn btn-primary' %>
  </div>

<% end %>


	

</div>